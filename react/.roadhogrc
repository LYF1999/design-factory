{
  "entry": "src/index.js",
  "disableCSSModules": true,
  "extraBabelPlugins": [
    ["import", { "libraryName": "antd", "style": "style" }],
    "transform-decorators-legacy",
    "mobx-deep-action"
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime"
      ],
      "proxy": {
        "/api": {
          "target": "http://localhost:8003/api/",
          "changeOrigin": true,
          "pathRewrite": { "^/api": "" }
        }
      }
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime"
      ]
    }
  }
}
